{
  "name": "BlueFox",
  "version": "0.0.1",
  "scripts": {
    "css": "compass watch",
    "build": "concurrently \"compass compile --force\" \"webpack -p\"",
    "build-dev": "concurrently \"compass compile --force\" \"webpack ---dev\"",
    "start": "concurrently \"compass watch\" \"webpack -w --color -p\"",
    "dev": "concurrently \"compass watch\" \"set NODE_ENV=dev&&webpack -w\"",
    "js": "webpack -w -p",
    "debug": "set NODE_ENV=dev&&webpack -w"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "owl.carousel": "^2.3.1",
    "popper.js": "^1.12.5",
    "winwheel": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.6",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "bxslider": "^4.2.13",
    "concurrently": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "imports-loader": "^0.6.5",
    "jquery-slimscroll": "^1.3.8",
    "uglify-js": "^2.7.4",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
